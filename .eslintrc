{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "import",
    "promise",
    "typescript"
  ],
  "rules": {
    "semi": 2,
    "no-duplicate-imports": "off",
    "no-unused-vars": "off",
    "import/no-duplicates": "error"
  }
}
